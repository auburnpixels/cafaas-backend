{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "coconutcraig/laravel-postmark": "^3.0",
        "doctrine/dbal": "^3.3",
        "firebase/php-jwt": "^6.2",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2",
        "laravel-validation-rules/credit-card": "^1.7",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.24",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "*",
        "mailerlite/mailerlite-api-v2-php-sdk": "^0.3.2",
        "moneyphp/money": "^4.1",
        "netflie/whatsapp-cloud-api": "^2.2",
        "securetrading/stpp_json": "^4.0",
        "sentry/sentry-laravel": "^4.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-honeypot": "^4.4",
        "spatie/laravel-image-optimizer": "^1.8.0",
        "spatie/laravel-newsletter": "^5.0",
        "spatie/laravel-sitemap": "^7.0",
        "spatie/laravel-sluggable": "^3.6",
        "srmklive/paypal": "~3.0",
        "stevebauman/location": "^7.0",
        "twilio/sdk": "^7.13",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^4.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
